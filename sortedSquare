import java.util.*;

class Program {
  public int[] sortedSquaredArray(int[] array) {
    // Write your code here.  ===============wrong===============
    int[] ans=new int[array.length];
         for(int i =0;i<array.length;i++)
         {
            ans[i]=array[i]*array[i];
           } 
    return ans;
  }
}
