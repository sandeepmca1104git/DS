import java.util.*;

class Program {
  public int[][] transposeMatrix(int[][] matrix) {
    // Write your code here.
      int row = matrix.length;
        int col = matrix[0].length;

        int[][] ans = new int[col][row];
        for (int i = 0; i < row; i++) {
            for (int j = 0; j < col; j++) {
                // System.out.print("###-"+matrix[i][j]);
                ans[j][i] = matrix[i][j];
            }
        }
        return ans;
}}
